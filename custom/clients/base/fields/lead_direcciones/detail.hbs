<h4>Direcciones</h4>
<table class="table table-striped table-condensed">
  <thead>
    <tr>
      <th>Tipo</th>
      <th>Tipo de direcci&oacute;n</th>
      <th>Calle</th>
      <th>Num Exterior</th>
      <th>Num Interior</th>
    </tr>
  </thead>
  <tbody>
    {{#each oDirecciones.direccion}}
    <tr>
      <td>{{getUnifinListValue 'tipodedirecion_list' tipodedireccion}}</td>
      <td>{{getUnifinListValue 'dir_Indicador_list' indicador}}</td>
      <td>{{calle}}</td>
      <td>{{numext}}</td>
      <td>{{numint}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>