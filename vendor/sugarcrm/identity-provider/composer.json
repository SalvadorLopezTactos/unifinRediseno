{
    "name": "sugarcrm/identity-provider",
    "version": "3.2.5",
    "description": "SugarCRM Identity Provider library",
    "type": "project",
    "license": "proprietary",
    "homepage": "http://www.sugarcrm.com",
    "support": {
        "wiki": "http://support.sugarcrm.com",
        "forum": "https://community.sugarcrm.com/sugarcrm",
        "irc": "irc://irc.freenode.org/sugarcrm"
    },
    "autoload": {
        "psr-4": {
            "Sugarcrm\\IdentityProvider\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sugarcrm\\IdentityProvider\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.3",
        "symfony/http-foundation": "^5.3",
        "symfony/security-core": "^5.3",
        "symfony/ldap": "^5.3",
        "psr/log": "^1.1",
        "onelogin/php-saml": "~3.1.0",
        "league/oauth2-client": "^2.7.0",
        "web-token/jwt-framework": "2.2.11",
        "guzzlehttp/guzzle": "^6.5.8|^7.7.0",
        "paragonie/random_compat": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.5",
        "friendsofphp/php-cs-fixer": "^2.1"
    },
    "minimum-stability": "stable"
}
