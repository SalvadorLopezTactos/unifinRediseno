{{#if this.dataFetched}}
    <div class="tab-pane active">
    {{#if recentCollection.length}}
        {{#eq this.activeTab 'recent-product'}}
        <ul class="unstyled listed recent-records-list">
        {{#each recentCollection.models}}
            <li class="recent-records">
                <a data-record-id="{{fieldValue this "id"}}" href="javascript:void(0)">
                    <i class="fa fa-list-alt"></i>
                    <span class="recent-link" rel="tooltip" data-original-title="{{fieldValue this "name"}}">{{fieldValue this "name"}}</span>
                </a>
            </li>
        {{/each}}
        </ul>
        {{else}}
            <ul class="unstyled listed recent-records-list">
            {{#each recentCollection.models}}
                <li class="recent-records">
                    <a data-record-id="{{fieldValue this "id"}}" href="javascript:void(0)">
                        <i class="fa fa-list-alt"></i>
                        <span class="recent-link" rel="tooltip" data-original-title="{{fieldValue this "name"}}">{{fieldValue this "name"}}</span>
                    </a>
                </li>
            {{/each}}
            </ul>
            {{#notEq this.pageNumList.pageNum '1'}}
                <div class="block-footer">
                    <nav class="pagination" aria-label="page-navigation">
                    <ul>
                        <li class="favorite-pagination">
                            <a class="paginate-aria-button nav-previous btn btn-link btn-invisible {{#if this.isPrevDisabled}}disabled{{/if}}" data-page-id="{{this.pageNumClicked}}" data-action="{{#unless this.isPrevDisabled}}page-nav-clicked{{/unless}}" aria-label="Previous">
                                <span class="previous-fav fa fa-chevron-left {{#if this.isPrevDisabled}}disabled{{/if}}" data-page-id="{{this.pageNumClicked}}" aria-hidden="true"></span>
                            </a>
                        </li>
                    {{#each this.pageNumList}}
                        <li class="{{listClass}}">
                            {{#if isIcon}}
                                <i class="{{subListClass}}"></i>
                            {{else}}
                                <a class="{{subListClass}} {{#if ../../this.isPageNumDisabled}}disabled{{/if}} {{#if isActive}}active{{/if}}"
                                   data-page-id="{{pageNum}}" data-action="page-clicked">{{pageNum}}</a>
                            {{/if}}
                        </li>
                    {{/each}}
                        <li class="favorite-pagination">
                            <a class="paginate-aria-button nav-next btn btn-link btn-invisible {{#if this.isNextDisabled}}disabled{{/if}}" data-page-id="{{this.pageNumClicked}}" data-action="{{#unless this.isNextDisabled}}page-nav-clicked{{/unless}}" aria-label="Next">
                                <span class="next-fav fa fa-chevron-right {{#if this.isNextDisabled}}disabled{{/if}}" data-page-id="{{this.pageNumClicked}}" aria-hidden="true"></span>
                            </a>
                        </li>
                    </ul>
                    </nav>
                </div>
            {{/notEq}}
        {{/eq}}
    {{else}}
        <div class="no-data">{{str "LBL_NO_DATA_AVAILABLE" this.module}}
        </div>
    {{/if}}
{{else}}
        <div class="loading-data">{{loading 'LBL_ALERT_TITLE_LOADING'}}</div>
    </div>
{{/if}}
